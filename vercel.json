{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "methods": ["GET", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://mjo-at-github.github.io",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      },
      "dest": "/api/$1"
    }
  ]
}